{
    "name": "dreamfactory/php-utils",
    "description": "A library of helper classes for PHP",
    "homepage": "https://www.dreamfactory.com/",
    "keywords": ["utility", "console", "dreamfactory"],
    "license": ["Apache-2.0"],
    "authors": [
        {
            "name": "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "kisma/kisma": "*",
        "symfony/dependency-injection": "*",
        "dreamfactory/enterprise-storage": "dev-develop as dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "DreamFactory\\Library\\Utility\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
